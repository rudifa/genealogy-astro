{
    "name": "genealogy-astro",
    "type": "module",
    "version": "1.3.1",
    "description": "Interactive family tree visualization and management application built with Astro",
    "author": {
        "name": "Rudi Farkas",
        "email": "rudifarkas@gmail.com"
    },
    "license": "MIT",
    "keywords": [
        "genealogy",
        "family-tree",
        "family-history",
        "astro",
        "typescript",
        "visualization",
        "graphviz",
        "interactive",
        "ancestors",
        "descendants"
    ],
    "homepage": "https://github.com/rudifarkas/genealogy-astro#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/rudifarkas/genealogy-astro.git"
    },
    "bugs": {
        "url": "https://github.com/rudifarkas/genealogy-astro/issues"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "dev": "astro dev --host",
        "start": "astro dev --host",
        "build": "astro check && astro build",
        "preview": "astro preview",
        "astro": "astro",
        "check": "astro check",
        "lint": "astro check",
        "clean": "rm -rf dist .astro",
        "test": "node --test 'test/**/*.test.js'",
        "test:forest-data": "node test/GenealogyForestData.test.js",
        "test:tree-data": "node test/GenealogyTreeData.test.js",
        "test:manual": "echo 'Manual tests are in the manual/ directory. Run them individually with node manual/<filename>.js'"
    },
    "dependencies": {
        "@astrojs/check": "^0.9.4",
        "astro": "^5.10.1"
    },
    "devDependencies": {
        "typescript": "^5.8.3"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
