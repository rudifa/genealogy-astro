{
    "name": "genealogy-astro",
    "type": "module",
    "version": "1.4.4",
    "description": "Interactive family tree visualization and management application built with Astro",
    "author": {
        "name": "Rudi Farkas",
        "email": "rudifarkas@gmail.com"
    },
    "license": "MIT",
    "keywords": [
        "genealogy",
        "family-tree",
        "family-history",
        "astro",
        "typescript",
        "visualization",
        "graphviz",
        "interactive",
        "ancestors",
        "descendants"
    ],
    "homepage": "https://github.com/rudifarkas/genealogy-astro#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/rudifarkas/genealogy-astro.git"
    },
    "bugs": {
        "url": "https://github.com/rudifarkas/genealogy-astro/issues"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "astro": "astro",
        "build": "astro check && astro build",
        "check": "astro check",
        "clean": "rm -rf dist .astro",
        "dev": "astro dev --host",
        "dev:check": "concurrently \"npm run dev\" \"npm run check -- --watch\"",
        "lint": "astro check",
        "preview": "astro preview",
        "start": "astro dev --host",
        "test": "node --test 'test/**/*.test.js' && npm run test:e2e",
        "test:e2e": "cypress run",
        "test:e2e:ci": "start-server-and-test dev http://localhost:4321 'cypress run --headless'",
        "test:e2e:dev": "start-server-and-test dev http://localhost:4321 'cypress open'",
        "test:e2e:headless": "cypress run --headless",
        "test:e2e:open": "cypress open",
        "test:forest-data": "node test/GenealogyForestData.test.js",
        "test:manual": "echo 'Manual tests are in the manual/ directory. Run them individually with node manual/<filename>.js'",
        "test:tree-data": "node test/GenealogyTreeData.test.js"
    },
    "dependencies": {
        "@astrojs/check": "^0.9.4",
        "astro": "^5.11.0"
    },
    "devDependencies": {
        "@cypress/vite-dev-server": "^6.0.3",
        "cypress": "^14.5.1",
        "start-server-and-test": "^2.0.12",
        "typescript": "^5.8.3"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
